{
    "name": "solfegejs",
    "description": "The Solfege framework",
    "homepage": "http://solfegejs.com",
    "version": "2.1.8",
    "author": "neolao <contact@neolao.com>",
    "private": false,
    "repository": "https://github.com/neolao/solfege.git",
    "license": "MIT",
    "keywords": [
        "framework",
        "solfege",
        "symfony",
        "dependency",
        "injection"
    ],
    "files": [
        "lib",
        "interface.js"
    ],
    "main": "lib/index.js",
    "engines": {
        "node": ">= 5"
    },
    "dependencies": {
        "co": "^4.6",
        "is-generator": "^1.0.3",
        "solfegejs-cli": "2.x",
        "solfegejs-configuration": "1.x",
        "solfegejs-dependency-injection": "1.x"
    },
    "devDependencies": {
        "babel-cli": "^6.7",
        "babel-preset-solfege": "^1.0",
        "chai": "^3.5",
        "co-mocha": "^1.2",
        "coveralls": "^2.13.1",
        "flow-bin": "^0.45.0",
        "gnomon": "^1.5.0",
        "jsdoc": "^3.4",
        "mocha": "^3.3",
        "nyc": "^10.3.2",
        "watch": "^1.0.2"
    },
    "scripts": {
        "prepare": "npm build && npm test",
        "test": "mocha --harmony --compilers js:babel-core/register --require co-mocha --recursive --reporter spec --bail",
        "coverage": "nyc npm test",
        "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
        "prebuild": "flow",
        "build": "babel --out-dir lib src --copy-files",
        "watch": "watch 'echo \"----------\"; npm run build | gnomon' ./src",
        "example-build": "flow; babel --out-dir example/lib example/src --copy-files",
        "example-console": "node --harmony  example/lib/console.js"
    },
    "babel": {
        "presets": [
            "solfege"
        ],
        "comments": false,
        "plugins": []
    }
}
